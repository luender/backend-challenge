generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  user     String @unique
  password String
}

model Room {
  id   String @id @default(uuid())
  user String
  room String
}

model Message {
  id       String @id @default(uuid())
  content  String
  username String
  room     String
}
