generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String
}

model Channel {
  id   String @id @default(uuid())
  name String
}

model Message {
  id       String @id @default(uuid())
  content  String
  username String
  channel  String
}
